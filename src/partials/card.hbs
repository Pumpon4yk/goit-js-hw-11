{{#each this}}
<a class="photo-card" href="{{largeImageURL}}" onclick="return false;">
    <img class="foto"  src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
    <div class="info">
        <p class="info-item">
            <b>Likes</b>
            {{likes}}
        </p>
        <p class="info-item">
            <b>Views</b>
            {{views}}
        </p>
        <p class="info-item">
            <b>Comments</b>
            {{comments}}
        </p>
        <p class="info-item">
            <b>Downloads</b>
            {{downloads}}
        </p>
    </div>
</a>
{{/each}}